<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<!-- ASP.NET Core runtime already included via Microsoft.NET.Sdk.Web -->
		<!-- Memory cache: you can rely on shared framework, but explicit package is ok -->
		<PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="9.0.10" />
		<PackageReference Include="Swashbuckle.AspNetCore" Version="9.0.6" />
		<!-- Remove old Microsoft.AspNetCore.Mvc.Core 2.x (incompatible). Use built-in controllers -->
	</ItemGroup>

</Project>
